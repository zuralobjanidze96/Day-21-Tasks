#include <iostream>

using namespace std;

int main()
{
    float r;
    cout << "შეიყვანეთ წრის რადიუსის სიგრძე: ";
    cin >> r;
    
    if (r>0) {
        cout << endl << "წრეწირის ფართობი ტოლია " << 3.141592653589793238462643383279502884197169399375105820974944592307816406286 * (r*r);
        cout << "-ის";
    } else {
        cout << endl << "რადიუსის მნიშვნელობა მეტი უნდა იყოს 0-ზე";
    }
    
    return 0;
}

